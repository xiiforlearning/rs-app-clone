{
  "name": "rsapp_clone",
  "version": "1.0.0",
  "description": "clone of rsapp",
  "main": "app.js",
  "scripts": {
    "start": "node app.js && nodemon index.js --prefix server2 && nodemon index.js --prefix server",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "schedule": "nodemon index.js --prefix server",
    "info": "nodemon index.js --prefix server2",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run schedule\" \"npm run info\"",
    "build": "cd client && npm run build",
    "install-client": "cd-client && npm install",
    "heroku-postbuild":"npm run install-client && npm run build"
  },
  "keywords": [
    "MERN"
  ],
  "author": "xii <xiiforlearning@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.14",
    "mongoose-unique-validator": "^2.0.3",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
